{
  "name": "swagger-http-codegen",
  "version": "0.0.8",
  "main": "./dist/index",
  "typings": "./dist/",
  "author": "Rinongusi <phatphamm01@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/phatphamm01/swagger-http-codegen.git",
    "type": "git"
  },
  "keywords": [
    "swagger",
    "http",
    "typescript",
    "generator",
    "codegen"
  ],
  "scripts": {
